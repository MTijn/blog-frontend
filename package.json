{
  "name": "blog-frontend",
  "version": "1.1.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MTijn/blog-frontend"
  },
  "contributors": [
    "Martijn Klene"
  ],
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "start": "ng serve",
    "test": "npm run env -s && ng test",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "e2e": "ng e2e",
    "start:fr": "ng serve --configuration=fr",
    "build:fr": "ng build --configuration=production-fr",
    "extract": "ng xi18n --output-path=locale",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng run blog-frontend:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/animations": "^9.1.1",
    "@angular/common": "^9.1.1",
    "@angular/compiler": "^9.1.1",
    "@angular/core": "^9.1.1",
    "@angular/platform-browser": "^9.1.1",
    "@angular/platform-browser-dynamic": "^9.1.1",
    "@angular/router": "^9.1.1",
    "@angular/upgrade": "^9.1.1",
    "@nguniversal/common": "^8.2.6",
    "@nguniversal/express-engine": "^8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
    "@ngx-lite/json-ld": "^0.6.2",
    "angular-in-memory-web-api": "^0.9.0",
    "bootstrap": "^4.4.1",
    "codelyzer": "^5.1.2",
    "core-js": "^3.6.2",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "rxjs": "^6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/cli": "^9.1.0",
    "@angular/compiler-cli": "^9.1.1",
    "@angular/platform-server": "^9.1.1",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "jasmine-core": "^3.5.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "lodash": "^4.17.15",
    "phantomjs-prebuilt": "^2.1.16",
    "protractor": "^5.4.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "3.8.3",
    "webpack-cli": "^3.3.10"
  }
